<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Member">

	<insert id="insert" parameterType="Member">
		insert into member(member_id,user_id,name,phone,password,email_id,email_server,zipcode,addr,addr_detail) 
		values(seq_member.nextval,#{user_id},#{name},#{phone},#{password},#{email_id},#{email_server},#{zipcode},#{addr},#{addr_detail})
	</insert>
	
	<select id="checkOverId" parameterType="String" resultType="int">
		select count(*) from member where user_id=#{uer_id}
	</select>
	
</mapper>